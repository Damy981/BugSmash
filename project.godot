; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "CanvasLayer",
"class": "BlockingDialogBox",
"language": "GDScript",
"path": "res://addons/blocking_dialog_box/blocking_dialog_box.gd"
}, {
"base": "CanvasLayer",
"class": "BlockingInputBox",
"language": "GDScript",
"path": "res://addons/blocking_dialog_box/blocking_input_box.gd"
}, {
"base": "CanvasLayer",
"class": "BlockingListSelection",
"language": "GDScript",
"path": "res://addons/blocking_dialog_box/blocking_list_selection.gd"
} ]
_global_script_class_icons={
"BlockingDialogBox": "",
"BlockingInputBox": "",
"BlockingListSelection": ""
}

[application]

config/name="BugSmash"
run/main_scene="res://Scenes/MainMenu.tscn"

[autoload]

ScreenManager="*res://Scripts/Singletons/ScreenManager.gd"
Pause="*res://Scripts/Singletons/Pause.gd"
PlayerProfile="*res://Scripts/Singletons/PlayerProfile.gd"
Quests="*res://Scripts/Singletons/Quests.gd"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "blocking_dialog_box" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[input]

pause={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777217,"unicode":0,"echo":false,"script":null)
 ]
}

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://demo/default_env.tres"
quality/2d/use_pixel_snap=true
